<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="80.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.312" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gmail.vleynik.olad.ControlPanelController">
   <children>
      <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="40.0" prefWidth="640.0" spacing="10.0">
         <children>
            <Button id="playPauseButton" mnemonicParsing="false" onAction="#playPause" text="Play/Pause">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
            <Button id="replayButton" mnemonicParsing="false" onAction="#replay" text="Replay" />
            <Region prefWidth="375.0" />
            <Button id="restartButton" mnemonicParsing="false" onAction="#restart" text="Restart player" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="200.0" spacing="10.0">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="File:" />
            <TextField fx:id="videoName" editable="false" prefWidth="200.0" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="viewed: " />
            <Text fx:id="viewCount" strokeType="OUTSIDE" strokeWidth="0.0" text="0">
               <HBox.margin>
                  <Insets left="-10.0" right="-10.0" />
               </HBox.margin>
            </Text>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text=" time(s)" />
            <Button mnemonicParsing="false" text="Open new file" onAction="#openNew">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
         </children>
      </HBox>
   </children>
</VBox>
